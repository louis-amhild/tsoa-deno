{
  "editor.codeActionsOnSave": {
    "eslint.autoFixOnSave": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "deno.cacheOnSave": true,
  "deno.codeLens.references": true,
  "deno.codeLens.implementations": true,
  "deno.codeLens.referencesAllFunctions": true,
  "deno.codeLens.test": true,
  "deno.enable": false,
  "deno.enablePaths": [
    "./deno-example",
    "./tests/fixtures/hono",
    "./packages/runtime/src/routeGeneration/templates/hono",
    "./tests/integration/hono-server.test.ts",
    "./tests/integration/hono-server-no-additional-allowed.test.ts",
    "./tests/fixtures/controllers/middlewaresHonoController.ts"
  ],
  "deno.importMap": "./tests/deno.import-map.json",
  "deno.config": "./tests/deno.json",
  "deno.testing.args": [
    "--allow-all",
    "--no-check",
    "--unstable-sloppy-imports"
  ],
  "deno.codeLens.testArgs": [
    "--allow-all",
    "--no-check",
    "--unstable-sloppy-imports",
    "--allow-net",
    "--allow-env",
    "--allow-read",
    "--config=./tests/deno.json"
  ],
  "deno.unstable": false
}
